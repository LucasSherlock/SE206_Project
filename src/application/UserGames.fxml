<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.UserGamesController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox maxWidth="1.7976931348623157E308" prefHeight="30.0" prefWidth="700.0">
               <children>
                  <Button fx:id="newList" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#move" styleClass="menu-button" text="Add List" HBox.hgrow="ALWAYS" />
                  <Button fx:id="edit" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#editList" styleClass="menu-button" text="Edit List" HBox.hgrow="ALWAYS" />
                  <Button fx:id="delete" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#deleteList" styleClass="menu-button" text="Delete List" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <ListView fx:id="QuestionList" prefHeight="319.0" prefWidth="700.0" VBox.vgrow="ALWAYS" />
            <HBox maxWidth="1.7976931348623157E308" minHeight="30.0" prefWidth="702.0">
               <children>
                  <Button fx:id="play" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#playGame" style="-fx-background-radius: 0;" styleClass="menu-button" text="Play" HBox.hgrow="ALWAYS" />
                  <Button fx:id="editQuestion" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#move" styleClass="menu-button" text="Edit Question" HBox.hgrow="ALWAYS" />
                  <Button fx:id="back" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#move" styleClass="menu-button" text="Back" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
