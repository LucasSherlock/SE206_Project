<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.UserSelectController">
   <children>
      <TextField fx:id="input" layoutX="257.0" layoutY="185.0" visible="false" />
      <Label fx:id="instructions" alignment="CENTER" layoutX="333.0" layoutY="97.0" text="Enter a new username:" visible="false" AnchorPane.leftAnchor="250.0" AnchorPane.rightAnchor="250.0" AnchorPane.topAnchor="150.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <VBox layoutX="50.0" layoutY="72.0" prefHeight="201.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="TOP_CENTER" fillHeight="false" maxHeight="-Infinity" minHeight="-Infinity" prefWidth="200.0" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="create" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" mnemonicParsing="false" onAction="#createUser" styleClass="menu-button" text="Create User" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="-1.0" right="-1.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="delete" mnemonicParsing="false" onAction="#deleteUser" styleClass="menu-button" text="Delete User" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <ListView fx:id="userList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
            <HBox alignment="BOTTOM_CENTER" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="50.0" prefHeight="75.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="confirm" maxHeight="1.7976931348623157E308" maxWidth="200.0" mnemonicParsing="false" onAction="#select" prefWidth="200.0" text="Confirm" HBox.hgrow="ALWAYS" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
