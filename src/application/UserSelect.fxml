<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UserController">
   <children>
      <ListView fx:id="userList" layoutX="205.0" layoutY="93.0" prefHeight="200.0" prefWidth="290.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="50.0" />
      <Button fx:id="confirm" layoutX="250.0" layoutY="350.0" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#select" prefHeight="50.0" prefWidth="200.0" text="Confirm" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="250.0" AnchorPane.rightAnchor="250.0" />
      <TextField fx:id="input" layoutX="257.0" layoutY="185.0" visible="false" />
      <Label fx:id="instructions" alignment="CENTER" layoutX="333.0" layoutY="97.0" text="Enter a new username:" visible="false" AnchorPane.leftAnchor="250.0" AnchorPane.rightAnchor="250.0" AnchorPane.topAnchor="150.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox layoutX="93.0" layoutY="100.0" prefHeight="50.0" prefWidth="200.0" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="create" mnemonicParsing="false" onAction="#createUser" prefHeight="50.0" prefWidth="200.0" text="Create User" />
            <Button fx:id="delete" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Delete User" />
         </children>
      </HBox>
   </children>
</AnchorPane>
